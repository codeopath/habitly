{
  "expo": {
    "name": "Habitly",
    "slug": "habitly",
    "version": "1.0.0",
    "scheme": "habitly",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./assets/Habitly_No_BG.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Habitly_No_BG.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Habitly_No_BG.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.habitly"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "098766a3-9490-4b00-8b62-329b530e7bb7"
      }
    },
    "owner": "sudiptod81"
  }
}
